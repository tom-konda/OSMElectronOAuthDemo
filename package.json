{
  "name": "osm-electron-oauth-demo",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "config": {
    "electron": "0.0.1"
  },
  "scripts": {
    "build": "node -e \"require('shelljs').exec('npm-run-all pug:build-* --parallel ts:build-* scss:build-* --serial webpack:* copy-js');\"",
    "copy-js": "node -e \"require('shelljs').cp('temp/main-proc/main.js', './')\"",
    "pug:build-renderer": "node -e \"require('shelljs').exec('pug -P -o ./renderer ./src/pug/renderer');\"",
    "scss:build-common": "node -e \"require('shelljs').exec('node-sass -r --output-style expanded -o ./renderer/css ./src/scss');\"",
    "ts:build-main-proc": "node -e \"require('shelljs').exec('tsc -p ./src/ts/main-proc');\"",
    "ts:build-renderer": "node -e \"require('shelljs').exec('tsc -p ./src/ts/renderer');\"",
    "ts:build-renderer-react-components": "node -e \"require('shelljs').exec('tsc -p ./src/ts/renderer-react-components');\"",
    "webpack:build-renderer": "node -e \"require('shelljs').exec('webpack --progress --colors --config ./webpack.config.js');\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "tom-konda",
  "license": "MIT",
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "devDependencies": {
    "@types/electron": "^1.3.20",
    "@types/node": "^6.0.38",
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.17",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "espower-typescript": "^5.0.1",
    "mocha": "^3.0.2",
    "node-localstorage": "^1.3.0",
    "node-sass": "^3.8.0",
    "npm-run-all": "^3.1.0",
    "oauth": "^0.9.14",
    "power-assert": "^1.4.1",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "shelljs": "^0.7.4",
    "should": "^11.1.0",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2"
  }
}