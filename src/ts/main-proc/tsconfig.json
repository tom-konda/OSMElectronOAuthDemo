{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "es2016",
            "dom"
        ],
        "types": [
            "node",
            "electron"
        ],
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noImplicitUseStrict": false,
        "noEmitOnError": true,
        "sourceMap": false,
        "outDir": "../../../temp/main-proc"
    },
    "include": [
        "main.ts",
        "**.d.ts",
        "app/**.ts"
    ],
    "exclude": []
}